<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/View"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/take_survey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="120dp"
        android:text="@string/take_survey"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="365dp" />

    <Button
        android:id="@+id/view_results"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="60dp"
        android:text="@string/view_results"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="430dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/top_text"
        android:text="@string/discover_your_values"
        android:textSize="24sp"
        android:textColor="@color/newColor"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/bottom_text"
        android:text="@string/identify_employers_values"
        android:textSize="24sp"
        android:textColor="@color/newColor"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="193dp" />
</RelativeLayout>